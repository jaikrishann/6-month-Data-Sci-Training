    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "url = \"https://github.com/torvalds\"\n",
    "res = requests.get(url)\n",
    "soup = BeautifulSoup(res.text, 'html.parser')\n",
    "\n",
    "repos = soup.find_all('span', class_='repo')\n",
    "for repo in repos:\n",
    "    print(repo.text.strip())"
